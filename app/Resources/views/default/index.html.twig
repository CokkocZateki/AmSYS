                    The footer of the box
                </div>--><!-- box-footer -->
            </div><!-- /.box -->
        </div>
        <div class="col-md-4">
            {{ include('elements/sitenotice.html.twig') }}
        </div>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"><i class="fa fa-balance-scale"></i> Outstanding Material Sales</h3>
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                    <table class="table table-striped" id="rSales">
                        <tr>
                            <th>Date</th>
                            <th>Order ID</th>
                            <th>Isk Amount</th>
                        </tr>
                        {% for sale in oSales %}
                            <tr>
                                <td>{{sale.created|date("m/d/Y H:i")}}</td>
                                <td>{{sale.orderid}}</td>
                                <td>{{sale.net|number_format(0, '.', ',')}}</td>
                            </tr>
                        {% endfor %}
                    </table>
                </div><!-- /.box-body -->
                <!--<div class="box-footer">
                    The footer of the box
                </div>--><!-- box-footer -->
            </div><!-- /.box -->
